{
  "name": "litmus",
  "version": "3.0.0-beta.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config config/webpack.dev.js",
    "build": "rm -rf dist && webpack --config config/webpack.prod.js",
    "lint": "eslint --ext .ts --ext .tsx src",
    "lint:staged": "sh scripts/eslint/eslint-staged.sh",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,scss}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,json,scss}\"",
    "test": "jest --maxWorkers=2",
    "test:silent": "jest --maxWorkers=2 --silent",
    "test:clean": "jest --clearCache",
    "test:watch": "yarn test --watch",
    "test:coverage": "jest --coverage --colors --maxWorkers=2",
    "prepare": "cd ../../ && husky install chaoscenter/web/.husky",
    "services": "node scripts/services.mjs",
    "strings:check": "node scripts/strings/yamlStringsCheck.js",
    "strings:sort": "yaml-sort -i src/strings/strings.en.yaml",
    "typecheck": "tsc",
    "typecheck:staged": "sh scripts/typecheck/typecheck-staged.sh",
    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "generate-certificate": "sh scripts/generate-certificate.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/litmuschaos/litmus"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/litmuschaos/litmus/issues"
  },
  "homepage": "https://github.com/litmuschaos/litmus#readme",
  "lint-staged": {
    "src/**/*.+(ts|tsx)": [
      "yarn lint:staged",
      "yarn typecheck:staged",
      "yarn format"
    ],
    "src/**/*.+(json)": [
      "yarn format"
    ],
    "strings.*.yaml": [
      "yarn strings:sort",
      "yarn strings:check"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml"
  },
